<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.metaaps.eoclipse.reader">
      <Reader
            Class="com.metaaps.eoclipse.vectordatareader.ShapeFileReader"
            Filter="*.shp"
            Type="Vector"
            name="Shape File Reader">
         <Format
               Type="VECTOR_ALL">
         </Format>
      </Reader>
      <Reader
            Class="com.metaaps.eoclipse.vectordatareader"
            Filter="*.csv"
            Type="Vector"
            name="CSV File Reader">
         <Format
               Type="VECTOR_POINT">
         </Format>
      </Reader>
   </extension>

</plugin>
